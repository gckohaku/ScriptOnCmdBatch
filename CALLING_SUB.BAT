@ECHO OFF
TITLE EXAMPLE
SETLOCAL ENABLEDELAYEDEXPANSION

CALL SUB/STRLEN "THIS,IS,TEST,TEXT" "," RES
SET RES.COUNT=%ERRORLEVEL%

IF %ERRORLEVEL% EQU 0 (
	GOTO LBL_LOOP_END
)

SET INDEX=0

:LBL_LOOP_TOP

CALL ECHO %INDEX%: %%RES.%INDEX%%%

SET /A INDEX=%INDEX% + 1

IF %INDEX% EQU %RES.COUNT% (
	GOTO LBL_LOOP_END
)

GOTO LBL_LOOP_TOP

:LBL_LOOP_END

ENDLOCAL
TITLE Command Prompt